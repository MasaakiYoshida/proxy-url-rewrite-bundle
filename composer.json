{
    "name": "phpmentors/proxy-url-rewrite-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle to rewrite URLs for applications behind reverse proxies",
    "keywords": ["symfony", "proxy", "url rewriting"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "KUBO Atsuhiro",
            "email": "kubo@iteman.jp"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/config": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/http-kernel": "~2.3",
        "symfony/routing": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "piece/stagehand-testrunner": "~4.1@dev",
        "symfony/browser-kit": "~2.3",
        "symfony/filesystem": "~2.3",
        "symfony/http-foundation": "~2.3",
        "symfony/twig-bridge": "~2.7",
        "symfony/twig-bundle": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "PHPMentors\\ProxyURLRewriteBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPMentors\\ProxyURLRewriteBundle\\": "tests/"
        }
    },
    "suggest": {
        "symfony/twig-bridge": ">=2.7.0 provides transparent URL rewriting with the `asset()` function in Twig templates",
        "symfony/twig-bundle": ">=2.7.0 provides transparent URL rewriting with the `asset()` function in Twig templates"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
